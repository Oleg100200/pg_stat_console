    LANGUAGE sql IMMUTABLE
    AS $_$ 		
   SELECT 		
      date_trunc('hour', $1)  +  cast(($2::varchar||' min') as interval) 		
      * round( (date_part('minute',$1)::float + date_part('second',$1)/ 60.)::float / $2::float )		
 $_$;


